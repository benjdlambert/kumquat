<div id="index-suite-container">
	<div class="panel panel-default">
	    <div class="panel-heading"><h3>Suites</h3></div>
	    <div class="panel-body">
	    	<table class="table table-striped table-hover">
	    	<thead>
		        <tr>
		            <th>Name</th>
		            <th>Enabled</th>
		            <th>No Of Tests</th>
		            <th>Last Run</th>
		        </tr>
		    </thead>
			{{#each suites}}
		    	<tr>
					<td><a href="suite/{{_id}}">{{name}}</a></td>
					<td></td>
					<td></td>
					<td></td>
				</tr>
			{{/each}}
			</table>
	    </div>
	</div>

	<div class="panel panel-default">
	    <div class="panel-heading"><h3>Live Progress</h3></div>
	    <div class="panel-body">
	    </div>
	</div>

	<div class="panel panel-default">
	    <div class="panel-heading"><h3>Previous 10 Test Runs</h3></div>
	    <div class="panel-body">
	    </div>
	</div>
</div>